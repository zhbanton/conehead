<table>
  <thead>
    <th>Start Date</th>
    <th>Total Output</th>
  </thead>
  <tbody>
    <% @production_schedules.each do |schedule| %>
      <tr>
        <td> <%=link_to schedule.starting_date.to_formatted_s(:long_ordinal), production_schedule_path(schedule)%> </td>
        <td> <%=schedule.total_output%> </td>
      </tr>
    <% end %>
  </tbody>
</table>
